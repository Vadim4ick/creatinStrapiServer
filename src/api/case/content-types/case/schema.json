{
  "kind": "collectionType",
  "collectionName": "cases",
  "info": {
    "singularName": "case",
    "pluralName": "cases",
    "displayName": "Кейсы",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "info": {
      "type": "string",
      "required": true
    },
    "imageMain": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "imageBig": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "case_names": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::case-name.case-name",
      "inversedBy": "cases"
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "image-blocks.double-image",
        "image-blocks.grid-image",
        "image-blocks.one-image",
        "image-blocks.text-block",
        "image-blocks.video",
        "image-blocks.about-project"
      ],
      "required": true
    },
    "mobileName": {
      "type": "string",
      "required": true
    },
    "backround": {
      "type": "customField",
      "options": [
        "white",
        "black"
      ],
      "required": true,
      "customField": "plugin::multi-select.multi-select"
    },
    "cases": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::case.case"
    },
    "imageMainMobile": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "imageBigMobile": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
