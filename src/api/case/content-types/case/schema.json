{
  "kind": "collectionType",
  "collectionName": "cases",
  "info": {
    "singularName": "case",
    "pluralName": "cases",
    "displayName": "Case",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "info": {
      "type": "string",
      "required": true
    },
    "imageMain": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "imageBig": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "Footer": {
      "type": "component",
      "repeatable": false,
      "component": "components.footer"
    },
    "case_name": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::case-name.case-name",
      "inversedBy": "cases"
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "image-blocks.double-image",
        "image-blocks.grid-image",
        "image-blocks.one-image",
        "image-blocks.text-block"
      ],
      "required": true
    }
