{
  "kind": "collectionType",
  "collectionName": "offers",
  "info": {
    "singularName": "offer",
    "pluralName": "offers",
    "displayName": "Спецпредложения",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "headingBanner": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "includes_blocks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::includes-block.includes-block"
    },
    "description": {
      "type": "text"
    },
    "highlighted": {
      "type": "string"
    },
    "img": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "offerBanner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::banner.banner"
    },
    "textBlocks": {
      "type": "component",
      "repeatable": true,
      "component": "components.text-block"
    },
    "sliderCase": {
      "type": "component",
      "repeatable": false,
      "component": "components.slider-case"
    },
    "service_name": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::service-name.service-name",
      "inversedBy": "offers"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "Title": {
      "type": "component",
      "repeatable": false,
      "component": "sections.section-titles"
    }
  }
}
