{
  "kind": "collectionType",
  "collectionName": "service_collections",
  "info": {
    "singularName": "service-collection",
    "pluralName": "service-collections",
    "displayName": "ServiceCollection",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "price": {
      "type": "biginteger",
      "required": true
    },
    "video": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "videos",
        "images"
      ]
    },
    "textBlocks": {
      "type": "component",
      "repeatable": true,
      "component": "components.text-block"
    },
    "banner": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "sliderCase": {
      "type": "component",
      "repeatable": false,
      "component": "components.slider-case"
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "deadlines": {
      "type": "string"
    },
    "Title": {
      "type": "component",
      "repeatable": false,
      "component": "sections.section-titles"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "Footer": {
      "type": "component",
      "repeatable": false,
      "component": "components.footer"
    },
    "contentBanner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::banner.banner"
    }
  }
}
