{
  "kind": "collectionType",
  "collectionName": "service_collections",
  "info": {
    "singularName": "service-collection",
    "pluralName": "service-collections",
    "displayName": "Подуслуги",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "price": {
      "type": "biginteger",
      "required": true
    },
    "video": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "videos",
        "images"
      ]
    },
    "textBlocks": {
      "type": "component",
      "repeatable": true,
      "component": "components.text-block"
    },
    "sliderCase": {
      "type": "component",
      "repeatable": false,
      "component": "components.slider-case"
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "deadlines": {
      "type": "string"
    },
    "Title": {
      "type": "component",
      "repeatable": false,
      "component": "sections.section-titles"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "contentBanner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::banner.banner"
    },
    "serviceColorImageComponent": {
      "type": "component",
      "repeatable": false,
      "component": "blocks.service-color-image"
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "services-content.single-image-block",
        "services-content.text-variant"
      ]
    }
  }
}
